{
  "name": "promise-finally",
  "version": "3.0.0",
  "description": "Simple wrapper to run promise \"finally\" logic",
  "main": "dist/promise-finally.js",
  "typings": "dist/promise-finally.d.ts",
  "files": [
    "dist/",
    "typings.json",
    "LICENSE"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "build": "npm run build-ts",
    "build-ts": "rm -rf dist/ && tsc",
