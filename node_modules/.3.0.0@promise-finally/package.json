    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/promise-finally/issues"
  },
  "homepage": "https://github.com/blakeembrey/promise-finally",
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "bluebird": "^3.0.5",
    "istanbul": "^0.4.3",
    "tap-dot": "^1.0.0",
    "tslint": "^3.10.2",
    "tslint-config-standard": "^1.0.0",
    "typescript": "^2.0.3",
